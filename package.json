    "@types/sequelize": "^4.28.8",
    "@types/react": "^16.9.19",
    "@types/react-alert": "^5.2.0",
    "@types/react-dom": "^16.9.5",
    "@types/shortid": "0.0.29",
    "@zeit/next-sass": "^1.0.1",
    "bcrypt": "^3.0.8",
    "bulma": "^0.8.0",
    "cross-env": "^7.0.0",
    "fastify": "^2.11.0",
